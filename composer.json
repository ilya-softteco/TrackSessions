{
  "name": "ilya_cig/track-session",
  "description": "Track Session",
  "autoload": {
    "psr-4": {
      "CIG\\TrackSession\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "CIG\\TrackSession\\Tests\\": "tests/"
    }
  },

  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Martinkevich",
      "email": "i.martinkevich@softteco.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },

  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CIG\\TrackSession\\Providers\\TrackSessionServiceProvider"
      ],
      "aliases": {
        "TrackSession": "CIG\\TrackSession\\Facades\\TrackSessionFacade"
      }

    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^9.2",
    "nunomaduro/phpinsights": "^2.11"
  }
}
